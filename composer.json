{
  "name": "arkitecht/laravel-b2b",
  "description": "Laravel Wrapper for B2B Rest integration library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron J. Rubin",
      "email": "aaron@arkitech.net"
    }
  ],
  "require": {
    "php": ">=5.6.4|^7",
    "arkitecht/b2b": "^2.1",
    "illuminate/support": "^5.0|^6.0|^7.0|^8.0"
  },
  "autoload": {
    "psr-4": {
      "Arkitecht\\B2B\\Laravel\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arkitecht\\B2B\\Laravel\\Providers\\B2bServiceProvider"
      ],
      "aliases": {
        "B2B": "Arkitecht\\B2B\\Laravel\\Facades\\B2BFacade",
        "B2BSoap": "Arkitecht\\B2B\\Laravel\\Facades\\B2BSoapFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
