{
  "name": "arkitecht/laravel-b2b",
  "description": "Laravel Wrapper for B2B Rest integration library",
  "license": "MIT",
  "authors": [
    {
      "name": "Aaron J. Rubin",
      "email": "aaron@arkitech.net"
    }
  ],
  "require": {
    "php": "^5.6",
    "arkitecht/b2b": "dev-master",
    "illuminate/support": "~5.4.0|~5.5.0"
  },
  "autoload": {
    "psr-4": {
      "Arkitecht\\B2B\\Laravel": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Arkitecht\\B2B\\Laravel\\B2bServiceProvider"
      ],
      "aliases": {
        "B2B": "Arkitecht\\B2B\\Laravel\\B2BFacade"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
